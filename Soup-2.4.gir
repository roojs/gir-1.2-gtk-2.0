          <type name="utf8" c:type="char*"/>
        </parameter>
        <parameter name="unescape_extra" transfer-ownership="none">
          <doc xml:whitespace="preserve">reserved characters to unescape (or %NULL)</doc>
          <type name="utf8" c:type="char*"/>
        </parameter>
      </parameters>
    </function>
